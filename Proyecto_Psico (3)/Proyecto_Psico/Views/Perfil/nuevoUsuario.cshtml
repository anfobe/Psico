@model Proyecto_Psico.Models.CrudModels.TablaUsuarios
@{
    ViewBag.Title = "Nuevo Usuario";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>nuevoUsuario</title>
</head>
<body>

    <h2>@ViewBag.Title</h2>



    @using (Html.BeginForm("nuevoUsuario", "Perfil", FormMethod.Post))
    {
        <div class="row">


            <div class="col-md-12">

                @Html.LabelFor(d => d.Idusuario)
                @Html.TextBoxFor(d => d.Idusuario, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Idusuario, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Idtipousuario)
                @Html.TextBoxFor(d => d.Idtipousuario, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Idtipousuario, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Cedula)
                @Html.TextBoxFor(d => d.Cedula, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Cedula, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Nombre)
                @Html.TextBoxFor(d => d.Nombre, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Nombre, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Apellido)
                @Html.TextBoxFor(d => d.Apellido, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Apellido, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Ultimavez)
                @Html.TextBoxFor(d => d.Ultimavez, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Ultimavez, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Correo)
                @Html.TextBoxFor(d => d.Correo, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Correo, null, new { @class = "danger" })
                <br />

            </div>
            <br />

            <div style="text-align:right">

                <input value="Guardar" class="btn btn-primary" type="submit" />

            </div>


        </div>

    }

    <br />
    <div style="text-align:left">

        <a class="btn btn-primary" href="~/Perfil/CRUD" role="button">Regresar</a>

    </div>


</body>
</html>
