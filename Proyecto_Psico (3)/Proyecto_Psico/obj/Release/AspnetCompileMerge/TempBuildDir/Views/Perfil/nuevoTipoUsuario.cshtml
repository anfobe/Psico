@model Proyecto_Psico.Models.CrudModels.TablaTipoUsuario
@{
    ViewBag.Title = "nuevo Tipo de Usuario";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>nuevoTipoUsuario</title>
</head>
<body>

    <h2>@ViewBag.Title</h2>



    @using (Html.BeginForm("nuevoTipoUsuario", "Perfil", FormMethod.Post))
    {
        <div class="row">


            <div class="col-md-12">

                @Html.LabelFor(d => d.Idusuario)
                @Html.TextBoxFor(d => d.Idusuario, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Idusuario, null, new { @class = "danger" })
                <br />

                @Html.LabelFor(d => d.Tipousuario)
                @Html.TextBoxFor(d => d.Tipousuario, "", new { @class = "form-control" })
                @Html.ValidationMessageFor(d => d.Tipousuario, null, new { @class = "danger" })
                <br />

            </div>
            <br />

            <div style="text-align:right">

                <input value="Guardar" class="btn btn-primary" type="submit" />

            </div>


        </div>

    }


    <br />
    <div style="text-align:left">

        <a class="btn btn-primary" href="~/Perfil/CRUD" role="button">Regresar</a>

    </div>

</body>
</html>
