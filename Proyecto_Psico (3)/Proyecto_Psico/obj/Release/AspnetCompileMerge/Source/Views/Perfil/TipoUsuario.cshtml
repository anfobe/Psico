
@model List<Proyecto_Psico.Models.CrudModels.TipoUsuario>
@{
    ViewBag.Title = "Mantenimiento Tipo de usuarios";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

    <title>Tipo de usuarios</title>
</head>
<body>

    <h2>@ViewBag.Title</h2>


    <div class="row" style="text-align:right;padding:5px">

        <a href="~/Perfil/nuevoTipoUsuario" class="btn btn-primary">Nuevo</a>

    </div>

    <div class="row">

        <div class="col-md-12">

            <table class="table">
                <tr>
                    <th>#</th>
                    <th>Tipo Usuario</th>
                    <th></th>
                </tr>

                @foreach (var oElemento in Model)
                {

                    <tr>

                        <td>@oElemento.Idusuario</td>
                        <td>@oElemento.Tipousuario</td>
                        <td>

                            <a class="btn btn-default" href="~/Perfil/EditarTipoUsuario/@oElemento.Idusuario">Editar</a>
                            <a class="btn btn-danger" href="~/Perfil/EliminarTipoUsuario/@oElemento.Idusuario">Eliminar</a>

                        </td>

                    </tr>


                }

            </table>




        </div>



    </div>


    <br />
    <div style="text-align:left">

        <a class="btn btn-primary" href="~/Perfil/CRUD" role="button">Regresar</a>

    </div>
</body>
</html>
